{
  "name": "synced",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "husky": "husky",
    "xcode:clean": "xcodebuild clean -workspace ios/synced.xcworkspace -scheme synced",
    "typescript": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "watchman": "watchman watch-del-all",
    "android:uninstall": "cd android && ./gradlew uninstallAll",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android:apk": "cd android && ./gradlew assembleRelease && cd ../",
    "android:bundle": "cd android && ./gradlew bundleRelease && cd ../",
    "splash-android": "yarn react-native generate-bootsplash splash.png --platforms=android --logo-width=184 --background=#fff --assets-output=src/assets/bootsplash",
    "splash-ios": "yarn react-native generate-bootsplash splash.png --platforms=ios --logo-width=264 --background=#fff --assets-output=src/assets/bootsplash",
    "components-types-schemas": "ts-to-zod ./node_modules/react-native/Libraries/StyleSheet/StyleSheetTypes.d.ts src/shared/themes/ui/lib/stylesheet.ts --skipValidation && scripts/schemas.sh",
    "asset": "npx react-native-asset && scripts/icons.sh",
    "api-codegen:compile": "bash scripts/api-codegen/compile-all.sh",
    "api-codegen:run": "bash scripts/api-codegen/run-codegen.sh",
    "svgo": "svgo -f ./src/assets/images -o ./src/assets/images",
    "endpoints": "npx openapi-typescript https://paste-your-swagger-link-here/api/docs/json -o src/shared/api/endpoints.ts && ./scripts/modify-endpoints.sh && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "pods": "cd ios && RCT_NEW_ARCH_ENABLED=1 && pod install",
    "prepare": "husky"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.1.1",
    "@gorhom/portal": "^1.0.14",
    "@hookform/resolvers": "4.1.0",
    "@react-navigation/bottom-tabs": "7.2.0",
    "@react-navigation/native": "7.0.14",
    "@react-navigation/native-stack": "7.2.0",
    "@shopify/react-native-skia": "1.11.8",
    "@tanstack/react-query": "5.66.7",
    "axios": "1.7.9",
    "dayjs": "1.11.13",
    "eventemitter3": "^5.0.1",
    "i18next": "23.15.1",
    "immer": "10.1.1",
    "lottie-react-native": "^7.2.2",
    "moti": "^0.30.0",
    "react": "18.3.1",
    "react-hook-form": "7.54.2",
    "react-i18next": "15.0.2",
    "react-native": "0.76.7",
    "react-native-awesome-slider": "2.5.3",
    "react-native-bootsplash": "6.3.3",
    "react-native-config": "1.5.5",
    "react-native-date-picker": "^5.0.12",
    "react-native-device-info": "14.0.4",
    "react-native-gesture-handler": "2.23.1",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-keyboard-controller": "1.16.8",
    "react-native-mmkv": "3.2.0",
    "react-native-modalfy": "3.6.0",
    "react-native-otp-entry": "^1.8.4",
    "react-native-reanimated": "patch:react-native-reanimated@npm%3A3.16.7#~/.yarn/patches/react-native-reanimated-npm-3.16.7-1e7cd6d376.patch",
    "react-native-safe-area-context": "5.1.0",
    "react-native-screens": "4.5.0",
    "react-native-svg": "15.11.1",
    "react-native-toast-message": "^2.3.0",
    "react-native-unistyles": "2.31.0",
    "react-native-vector-icons": "10.2.0",
    "sonner-native": "0.19.0",
    "ts-pattern": "^5.6.2",
    "zod": "3.24.2",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native/babel-preset": "0.76.7",
    "@react-native/eslint-config": "0.76.7",
    "@react-native/metro-config": "0.76.7",
    "@react-native/typescript-config": "0.76.7",
    "@svgr/cli": "^8.1.0",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.1.0",
    "husky": "9.0.11",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "3.0.3",
    "react-native-svg-transformer": "^1.5.0",
    "reactotron-react-native": "^5.1.9",
    "reactotron-react-native-mmkv": "^0.2.7",
    "reactotron-react-query": "^1.0.4",
    "ts-to-zod": "3.8.5",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.6.0"
}
